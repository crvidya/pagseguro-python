[run]
branch = True
source = pagseguro
include = pagseguro/*
omit = pagseguro/tests/*

[html]
title = Coverage Report for PagSeguro-Python

[report]
show_missing = True
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain if tests don't hit defensive assertion code:
    raise NotImplementedError